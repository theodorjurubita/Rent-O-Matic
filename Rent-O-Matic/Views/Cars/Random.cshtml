@using Rent_O_Matic.ViewModels
@model IEnumerable<Rent_O_Matic.ViewModels.Car>

@{
    ViewBag.Title = "Random";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table>
    <thead>
    <tr>
        <th>
            Brand
        </th>
        <th>
            Model
        </th>

    </tr>
    
    </thead>
    <tbody>
        @foreach (var car in Model)
        {
            <tr>
                <td>@car.Brand</td>
                <td>@car.Model</td>
            </tr>    
        }
    </tbody>
</table>

